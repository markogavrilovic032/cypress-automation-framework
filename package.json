{
  "name": "cypress-automation-framework",
  "version": "1.0.0",
  "description": "Cypress Automation Framework",
  "main": "index.js",
  "scripts": {
    "triggerAllTests-headless": "npx cypress run",
    "trriggerAllTests-headed": "npx cypress run --headed",
    "trriggerAllTests-chrome": "npx cypress run --browser chrome",
    "trriggerAllTests-dasboard": "npx cypress run --record --key dff68e9f-57bd-4f31-8e67-eff788bbe782",
    "trriggerAllTests-webdriver": "npx cypress run --spec 'cypress\\e2e\\webdriver-uni\\*'",
    "trrrigerAllTests-autostore-dashboard": "npx cypress run --spec 'cypress\\e2e\\automation-test-store\\*' --record --key dff68e9f-57bd-4f31-8e67-eff788bbe782",
    "toTestStore": "npx cypress run --spec 'cypress\\e2e\\automation-test-store\\*'",
    "junit-merge": "npx junit-merge -d cypress\\results\\junit -o cypress\\results\\junit\\results.xml",
    "delete-junit-report": "del cypress\\results\\junit\\results.xml",
    "delete-results": "rimraf cypress\\results\\*",
    "mochawesome-merge": "npx mochawesome-merge cypress\\results\\mochawesome\\*.json > mochawesome.json && npx marge mochawesome.json",
    "detete-mochawesome-report": "rimraf  mochawesome-report\\*",
    "cypress-webdriver-pack": "npm run delete-results && npm run detete-mochawesome-report && npm run trriggerAllTests-webdriver && npm run mochawesome-merge",
    "cypress-regression-pack": "npm run delete-results && npm run detete-mochawesome-report && npm run triggerAllTests-headless && npm run mochawesome-merge"
  },
  "author": "Marko Gavrilovic",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.3.0",
    "cypress-multi-reporters": "^1.6.3",
    "cypress-xpath": "^2.0.1",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
